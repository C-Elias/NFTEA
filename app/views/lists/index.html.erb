<div class="container">
  <%= link_to '< Back to my profile', user_path(current_user) %>
  <div class="top">
    <h4>My lists:</h4>
  </div>
  <div id="my-list-grid">
    <% @lists.each do |list| %>
    <div class="list-card">
      <img class="list-cover" src="<%= list.nfts[0].ext_url %>"alt="">
      <div class="p-3">
        <%= link_to user_list_path(list.user, list) do %>
          <h5><%= list.name %></h5>
        <% end %>
        <div class="w-100 text-right">
        <p>(<%= list.nfts.size %>) nfts</p>
        </div>
      </div>
    </div>
    <% end %>
  </div>
</div>
