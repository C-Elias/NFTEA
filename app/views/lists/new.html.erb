<div class="container">
  <%= link_to '< Back to my lists', user_lists_path(current_user)%>
  <div class="container py-5">
    <div class="row">
      <div class="col-12 col-md-6 offset-lg-3">
        <div class="bg-white rounded p-3 shadow">
          <h3>Create a new NFT</h3>
          <%= simple_form_for [current_user, @list] do |f| %>
            <%= f.input :name %>
            <%= f.submit %>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
