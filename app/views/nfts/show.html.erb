<div class="container">
  <%= link_to '< Back to NFTS', nfts_path %>
  <div class="nft-card-show">
    <div class="d-flex justify-content-between">
      <%= link_to do %>
        <p><%= @nft.user.email %></p>
      <% end %>
        <p>‚ù§ <%= rand(0..100) %></p>
      </div>
    <div class="d-flex">
      <img src="<%= @nft.ext_url %>" alt="" id="nft-foto-show">
      <div class="text-wrapper">
        <h1><%= @nft.name %></h1>
        <p><%= @nft.description %></p>
      </div>
    </div>
  </div>
</div>
